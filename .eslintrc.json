{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "next"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-empty-function": "off",
    "no-unused-vars": [
      "warn",
      {
        "args": "all",
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
